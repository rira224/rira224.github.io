	height: 100vh;				/*画面に対して高さ100%*/
	display: flex;				/*flexボックスを使う指定*/
	justify-content: center;	/*左右の中央に配置*/
	align-items: center;		/*天地中央に配置*/
	text-align: center;			/*文字をセンタリング*/
}

/*ヘッダーのリンクテキスト*/
